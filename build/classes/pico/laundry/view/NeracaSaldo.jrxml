	<queryString>
		<![CDATA[SELECT
	perkiraan.kode AS perkiraan_kode,
	perkiraan.nama AS perkiraan_nama,
	IF((SUM(jurnal_detail.debet) - SUM(jurnal_detail.kredit)) > 0, SUM(jurnal_detail.debet) - SUM(jurnal_detail.kredit), 0) AS saldo_debet,
    IF((SUM(jurnal_detail.debet) - SUM(jurnal_detail.kredit)) < 0, ABS(SUM(jurnal_detail.debet) - SUM(jurnal_detail.kredit)), 0) AS saldo_kredit
FROM
	perkiraan
LEFT JOIN
	jurnal_detail
ON
	jurnal_detail.perkiraan = perkiraan.kode
GROUP BY 
	perkiraan.kode
ORDER BY
	perkiraan.kode
	ASC]]>
	</queryString>
	<field name="perkiraan_kode" class="java.lang.Integer"/>
	<field name="perkiraan_nama" class="java.lang.String"/>
	<field name="saldo_debet" class="java.math.BigDecimal"/>
	<field name="saldo_kredit" class="java.math.BigDecimal"/>
	<variable name="total_debet" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{saldo_debet}]]></variableExpression>
	</variable>
	<variable name="total_kredit" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{saldo_kredit}]]></variableExpression>
	</variable>
