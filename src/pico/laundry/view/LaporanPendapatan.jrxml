<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="report name" pageWidth="595" pageHeight="842" columnWidth="535" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="75af24e2-25b5-442f-a687-feb06591ee73">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="tanggalAwal" class="java.util.Date"/>
	<parameter name="tanggalAkhir" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT 
    pelanggan.id AS pelanggan_id, 
    pelanggan.nama AS pelanggan_nama, 
    pelanggan.alamat AS pelanggan_alamat, 
    pelanggan.telepon AS pelanggan_telepon, 
    pemesanan.id_pemesanan AS pemesanan_nomor, 
    pemesanan.pesanan_masuk AS pemesanan_tanggal, 
    pemesanan.total_biaya AS pemesanan_total, 
    pemesanan_detail.harga AS detail_harga, 
    pemesanan_detail.item AS pemesanan_item,
    pemesanan_detail.jumlah AS detail_jumlah
FROM 
    pemesanan_detail 
LEFT JOIN 
    pemesanan 
    ON pemesanan.id_pemesanan = pemesanan_detail.id_pemesanan
LEFT JOIN 
    pelanggan 
    ON pelanggan.id = pemesanan.pelanggan 
WHERE 
    pemesanan.pesanan_masuk >= $P{tanggalAwal}
    AND pemesanan.pesanan_masuk <= $P{tanggalAkhir}
ORDER BY 
    pemesanan.id_pemesanan ASC;
]]>
	</queryString>
